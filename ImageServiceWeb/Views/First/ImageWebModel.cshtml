@model ImageServiceWeb.Models.ImageWebModel
@{
    ViewBag.Title = "ImageWebModel";
}
<head>
    <style>
        h1 {
            color: red;
            font-weight: bold;
        }
    </style>
</head>

<h1>Image Web</h1>

<br />
<div>
    @Html.LabelFor(model => model.ServiceStatus)
    @Html.DisplayFor(model => model.ServiceStatus)
</div>
<div>
    @Html.LabelFor(model => model.NumOfPhotos)
    @Html.DisplayFor(model => model.NumOfPhotos)
</div><br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.students)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
        </tr>
    }

</table>